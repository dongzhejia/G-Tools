# xtool相关的项目设置(YAML语法)

TaskSettings:
    PackUIImage:
        outputdir: asset/ui
        index_name: index.json
        index_fmt: json
    ExportSpine:
        outputdir: asset/anim
        index_name: index.json
        dependent: ['PackUIImage']
    AnimMainfestTask:
        outputdir: script/assetscript

cleanup:
    ui: [asset/ui]

target: [dev, iOS, Android, Android_HP, Android_565]
